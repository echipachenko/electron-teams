{
  "name": "teams-ubuntu",
  "version": "2.0.1",
  "description": "Microsoft teams for ubuntu platforms",
  "main": "src/js/main.js",
  "scripts": {
    "start": "electron .",
    "dist": "rm -rf dist && npm run 64-bit",
